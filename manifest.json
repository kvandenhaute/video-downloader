{
  "name": "Video Downloader",
  "description": "Download videos",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "downloads"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
